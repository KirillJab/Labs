//---------------------------------------------------------------------------
#include "Line.h"
#ifndef ListH
#define ListH
//---------------------------------------------------------------------------

class List
{
	public:
	Line* Head;
	Line* Tail;
	int Size;
	List()
	{
		Head = NULL;
		Tail = NULL;
		Size = 0;
	}
	void add(string);
	Line* get(int);
};



#endif
