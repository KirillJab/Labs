

#include <vcl.h>
#pragma hdrstop

#include "Programm.h"
#pragma package(smart_init)
#pragma resource "*.dfm"
TForm1 *Form1;
struct Line
{
	Line* prev;
	Line* next;
	string text;
}Line;

struct List
{
	 struct Line* head;
	 struct Line* tail;
}List;

struct List createList(char text[])
{
	struct List list;
	list.head = NULL;
	list.tail = NULL;
	return list;
}
__fastcall TForm1::TForm1(TComponent* Owner)
	: TForm(Owner)
{
	struct Line line1;
	line1.text = "Hello";
	line1.prev = NULL;
}

