//---------------------------------------------------------------------------

#pragma hdrstop

#include "Tool.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)

Tool FromString(string inp)
{
	Tool tool;
	string buff;
	int i = 0;

	for(; inp[i] != '\t'; i++)
	{
		tool.GroupName += inp[i];
	}
	i++;
	for(; inp[i] != '\t'; i++)
	{
		tool.GroupName += inp[i];
	}
	i++;
	for(; inp[i] != '\t'; i++)
	{
		tool.GroupName += inp[i];
	}
	i++;
	for(; inp[i] != '\t'; i++)
	{
		tool.GroupName += inp[i];
	}
	i++;
	for(; inp[i] != '\t'; i++)
	{
		tool.GroupName += inp[i];
	}
	i++;
	for(; inp[i] != '\0'; i++)
	{
		tool.GroupName += inp[i];
	}
	i++;
	return tool;
}


