//---------------------------------------------------------------------------

#pragma hdrstop

#include "Tool.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)

Tool FromString(string inp)
{
	Tool tool;
	int i = 0;
	for(; inp[i] != '\t';)
	{
		tool.GroupName += inp[i];
	}
	i++;
	for(; inp[i] != '\t';)
	{
		tool.GroupName += inp[i];
	}
	i++;
    for(; inp[i] != '\t';)
	{
		tool.GroupName += inp[i];
	}
	i++;
    for(; inp[i] != '\t';)
	{
		tool.GroupName += inp[i];
	}
	i++;
	for(; inp[i] != '\t';)
	{
		tool.GroupName += inp[i];
	}
	i++;
    for(; inp[i] != '\t';)
	{
		tool.GroupName += inp[i];
	}
	i++;
	return tool;
}


