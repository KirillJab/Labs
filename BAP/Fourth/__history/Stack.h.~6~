//---------------------------------------------------------------------------

#ifndef StackH
#define StackH
//---------------------------------------------------------------------------
class Item
{
public:
	char ch;
	Item* Prev;
	Item()
	{
        Prev = NULL;
    }
};
class Stack
{
public:
	Item* Tail;
	void Push(char);
	void Pop();
	Item* Back();
	bool Empty();
};
#endif
