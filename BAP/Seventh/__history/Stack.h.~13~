//---------------------------------------------------------------------------

#ifndef StackH
#define StackH
//---------------------------------------------------------------------------

class Item
{
	public:
    int key;
	Item* next;
	Item(int num)
	{
		next = NULL;
		key = num;
	}
};

class Stack
{
	public:
	Item* Tail;
	Stack()
	{
        Tail = NULL;
    }
	void Push(char);
	void Push(float);
	void Pop();
	Item* Back();
	bool Empty();
};
#endif
